<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.java.j2seproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2se-project/3">
            <name>SpatoBiz</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <source-roots>
                <root id="src.rsc.dir" name="Resource Packages"/>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>SpatoBizCommon</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
        <spring-data xmlns="http://www.netbeans.org/ns/spring-data/1">
            <config-files>
                <config-file>rsc/com/pos/spatobiz/app/resource/spring/application.ctx.xml</config-file>
                <config-file>rsc/com/pos/spatobiz/app/resource/spring/hibernate.ctx.xml</config-file>
                <config-file>rsc/com/pos/spatobiz/app/resource/spring/message.ctx.xml</config-file>
                <config-file>rsc/com/pos/spatobiz/app/resource/spring/panel.ctx.xml</config-file>
                <config-file>rsc/com/pos/spatobiz/app/resource/spring/spring.ctx.xml</config-file>
                <config-file>rsc/com/pos/spatobiz/app/resource/spring/button.ctx.xml</config-file>
            </config-files>
            <config-file-groups>
                <config-file-group name="all">
                    <config-file>rsc/com/pos/spatobiz/app/resource/spring/application.ctx.xml</config-file>
                    <config-file>rsc/com/pos/spatobiz/app/resource/spring/hibernate.ctx.xml</config-file>
                    <config-file>rsc/com/pos/spatobiz/app/resource/spring/message.ctx.xml</config-file>
                    <config-file>rsc/com/pos/spatobiz/app/resource/spring/panel.ctx.xml</config-file>
                    <config-file>rsc/com/pos/spatobiz/app/resource/spring/spring.ctx.xml</config-file>
                    <config-file>rsc/com/pos/spatobiz/app/resource/spring/button.ctx.xml</config-file>
                </config-file-group>
            </config-file-groups>
        </spring-data>
    </configuration>
</project>
